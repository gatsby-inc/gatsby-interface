{
  "name": "gatsby-interface",
  "version": "0.0.250",
  "description": "Component library for Gatsby projects",
  "source": "index.ts",
  "files": [
    "dist",
    "babel-plugin-gatsby-interface/index.js",
    "babel-plugin-gatsby-interface/README.md"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "jest --env=jest-environment-jsdom-sixteen --verbose",
    "test:a11y": "jest __tests__/AccessiblityStoryshots.test.js --testPathIgnorePatterns=[] --env=jest-environment-jsdom-sixteen",
    "test:watch": "jest --watch --env=jest-environment-jsdom-sixteen --verbose",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "tsc": "tsc",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch",
    "build": "yarn build:types && yarn build:rollup",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.build-types.json",
    "build:rollup": "rollup -c",
    "stylelint": "stylelint './src/**/*.js'",
    "format": "prettier --write",
    "format:all": "yarn format \"{src,.storybook}/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint --max-warnings=0 --fix --color --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives",
    "lint:all": "yarn lint src .storybook",
    "chromatic": "cross-env STORYBOOK_CHROMATIC=1 chromatic test",
    "chromatic:ci": "cross-env STORYBOOK_CHROMATIC=1 babel-node --presets @babel/env scripts/run-chromatic.js",
    "scaffold:component": "babel-node --presets @babel/env scripts/scaffold-component/scaffold-component.js",
    "scaffold:icon": "babel-node --presets @babel/env scripts/scaffold-icon/scaffold-icon.js",
    "prepublish": "yarn build",
    "release": "standard-version",
    "release:branch": "babel-node --presets @babel/env scripts/prerelease-branch.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/helper-module-imports": "^7.12.1",
    "@mdx-js/react": "^1.5.2",
