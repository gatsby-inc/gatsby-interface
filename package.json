    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.12.4",
    "formik": "^2.0.8",
    "fs": "^0.0.1-security",
    "gatsby": "2.6.0",
    "husky": "^1.3.1",
