{
  "name": "gatsby-interface",
  "version": "0.0.29",
  "description": "Component library for Gatsby projects",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "babel src -d dist",
    "dev": "watch 'yarn run build' src",
    "stylelint": "stylelint './src/**/*.js'",
    "format": "prettier-eslint --write \"src/**/*.js\" && eslint src --fix --color --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives",
    "lint:commit": "prettier-eslint --eslint-config-path=.eslintrc --write"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "peerDependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "gatsby": "2.6.0",
    "prop-types": "^15.6.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-icons": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@emotion/babel-preset-css-prop": "^10.0.14",
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@storybook/addon-a11y": "^5.0.11",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addon-storyshots": "^5.0.11",
    "@storybook/addon-storysource": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-emotion": "^10.0.14",
    "babel-plugin-react-docgen": "^2.0.2",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-gatsby": "^0.1.11",
    "dotenv": "^5.0.1",
    "eslint": "^5.13.0",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "gatsby": "2.6.0",
    "husky": "^1.3.1",
    "jest": "^22.4.3",
    "jest-dom": "^3.1.1",
    "jest-emotion": "^10.0.14",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1",
    "prettier-eslint-cli": "^4.7.1",
    "prop-types": "^15.6.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-icons": "^3.2.1",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^5.6.1",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^2.0.0",
    "stylelint-processor-styled-components": "^1.5.1",
    "unfetch": "^3.1.1",
    "watch": "^1.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,yml,yaml}": [
      "yarn lint:commit",
      "git add"
    ]
  }
}
