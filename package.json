  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,yml,yaml}": [
      "yarn lint:eslint",
      "yarn lint:prettier",
      "git add"
    ]
  }
