    "yup": "^0.27.0"
  },
  "resolutions": {
    "**/@types/react": "*",
    "**/@types/react-dom": "*",
    "jsdom": "16.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn lint",
      "yarn format",
      "git add"
    ],
    "*.{json,md,yml,yaml}": [
      "yarn format",
      "git add"
    ]
  }
