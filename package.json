    "date-fns": "^2.8.1",
    "fs-extra": "^9.0.1",
    "gatsby-design-tokens": "^2.0.2",
    "lodash.sample": "^4.2.1",
    "markdown-to-jsx": "^7.0.0",
    "theme-ui": "^0.2.49"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.14",
